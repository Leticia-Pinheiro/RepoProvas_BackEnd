generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model teachers {
  id       Int    @id @default(autoincrement())
  name     String @unique
}

model terms {
  id       Int    @id @default(autoincrement())
  number   Int    @unique
}

model categories {
  id       Int    @id @default(autoincrement())
  name     String @unique
}
